@define SYSNONE 0

@define SYS_READ 0
@define SYS_WRITE 1
@define SYS_OPEN 2
@define SYS_CLOSE 3

@define STDIN 0
@define STDOUT 1
@define STDERR 3


// Should be variadic
func long _syscall(long name, long rdi, long rsi, long rdx) {
    asm {
        "mov    rax, $name"
        "mov    rdi, $rdi"
        "mov    rsi, $rsi"
        "mov    rdx, $rdx"
        "syscall"
    }
}